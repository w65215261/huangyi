<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="hl.hyzx.client.dao.ServiceMenuDao">
	<select id="getServiceMenu" resultType="hl.hyzx.client.dto.ServiceMenuDTO">
		select *
		from servicemenu
		where service_Menu_Type=#{serviceMenuType}
	</select>
</mapper>